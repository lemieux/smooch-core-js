{
    "name": "smooch-core",
    "version": "8.11.4",
    "description": "Javascript wrapper for Smooch API",
    "homepage": "https://smooch.io",
    "main": "lib/smooch.js",
    "scripts": {
        "test": "BABEL_ENV=test mocha --compilers js:babel-core/register --require ./test-setup.js ./test/**/*.spec.js",
        "test-ci": "BABEL_ENV=test MOCHA_FILE=$GITHUB_ACTIONS_TEST_REPORTS/test-results.xml mocha --compilers js:babel-core/register -R mocha-junit-reporter --require ./test-setup.js ./test/**/*.spec.js",
        "build": "mkdir -p lib && rm -rf lib/* && babel -d lib/ src/",
        "release": "release",
        "docs": "jsdoc src/api -t node_modules/minami -c jsdoc.json",
        "lint": "eslint . --ext=js",
        "prettier:lint": "prettier '**/*.{js,jsx,ts,json,md}' --list-different"
    },
    "author": "Marc-Antoine Lemieux",
    "license": "Apache-2.0",
    "repository": {
        "type": "git",
        "url": "https://github.com/zendesk/sunshine-conversations-javascript/tree/v1-master"
    },
    "devDependencies": {
        "@smooch/eslint-plugin-smooch": "1.0.3",
        "babel": "6.5.2",
        "babel-cli": "6.9.0",
        "babel-core": "6.9.0",
        "babel-eslint": "6.0.4",
        "babel-plugin-rewire": "1.1.0",
        "babel-preset-es2015": "6.9.0",
        "babel-preset-es2015-loose": "7.0.0",
        "babel-preset-stage-2": "6.5.0",
        "chai": "4.0.2",
        "chai-as-promised": "7.0.0",
        "eslint": "4.18.2",
        "eslint-config-prettier": "4.1.0",
        "eslint-plugin-mocha": "5.3.0",
        "estraverse-fb": "1.3.1",
        "hat": "0.0.3",
        "jsdoc": "3.6.6",
        "minami": "1.1.1",
        "mocha": "2.3.3",
        "mocha-junit-reporter": "1.9.0",
        "prettier": "1.16.4",
        "should": "7.1.1",
        "sinon": "2.3.5",
        "sinon-chai": "2.11.0",
        "streamifier": "0.1.1",
        "uglifyify": "3.0.1",
        "uglifyjs": "2.4.10"
    },
    "dependencies": {
        "form-data": "0.2.0",
        "jsonwebtoken": "5.4.1",
        "node-fetch": "1.7.2"
    }
}
